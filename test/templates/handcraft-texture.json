      "template": "<section style='margin: 3.8em 0.08em; text-align: center; position: relative;'><section style='position: absolute; top: -13px; left: 22%; width: 28px; height: 29px; background: #c49670; border-radius: 51% 49% 48% 52%; opacity: 0.28;'></section><section style='display: inline-block; border-radius: 19px 17px 21px 18px; overflow: hidden; box-shadow: 0 8px 32px rgba(122, 79, 46, 0.18); background: #fefdf9; padding: 1.1em 1.3em 1.2em 1.15em; position: relative; z-index: 1; border: 2.5px solid #c49670;'><section style='border-radius: 14px 16px 13px 15px; overflow: hidden; position: relative; box-shadow: inset 0 1px 3px rgba(122, 79, 46, 0.08);'><img src='{{url}}' alt='{{alt}}' style='max-width: 100%; display: block;' /></section></section>{{?caption}}</section>",
