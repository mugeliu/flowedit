{
  "id": "warm-orange",
  "name": "新粗野橙潮主题",
  "description": "大胆几何与强烈对比的新粗野主义设计，极具视觉冲击力",
  "category": "新粗野主义",
  "author": "FlowEdit Team",
  "version": "2.0.0",
  "inlineStyles": {
    "strong": "font-weight: 900; color: #000000; background: #FF6B35; padding: 0.1em 0.3em; border: 2px solid #000000;",
    "b": "font-weight: 900; color: #000000; background: #FF6B35; padding: 0.1em 0.3em; border: 2px solid #000000;",
    "em": "font-style: italic; color: #FF4500; font-weight: 700; background: #FFFF00; padding: 0.05em 0.2em; border: 1px solid #000000;",
    "i": "font-style: italic; color: #FF4500; font-weight: 700; background: #FFFF00; padding: 0.05em 0.2em; border: 1px solid #000000;",
    "u": "text-decoration: none; color: #000000; background: #FF6B35; padding: 0.1em 0.2em; border-bottom: 4px solid #000000;",
    "code": "font-family: 'Courier New', monospace; font-size: 0.9em; background: #000000; color: #00FF00; padding: 0.3em 0.6em; border: 3px solid #FF6B35; font-weight: 700;",
    "mark": "background: #FFFF00; color: #000000; padding: 0.2em 0.4em; border: 2px solid #000000; font-weight: 700;",
    "a": "color: #FF4500; text-decoration: none; font-weight: 800; background: #FFFFFF; padding: 0.1em 0.2em; border: 2px solid #000000; box-shadow: 3px 3px 0px #000000;",
    "sup": "color: #FF6B35; font-size: 0.7em; font-weight: 900; background: #000000; padding: 0.1em 0.2em; vertical-align: super;"
  },
  "globalStyles": {
    "container": "<section style='max-width: 100%; margin: 0 auto; padding: 2em; font-size: 18px; line-height: 1.6; color: #000000; font-family: -apple-system, BlinkMacSystemFont, \"Helvetica Neue\", Arial, sans-serif; background: #FFFFFF; border: 5px solid #000000; box-shadow: 10px 10px 0px #FF6B35; font-weight: 600;'>{{content}}</section>",
    "references": "<section style='margin-top: 3em; padding: 2em; background: #FF6B35; border: 4px solid #000000; font-family: inherit; box-shadow: 8px 8px 0px #000000;'><section style='display: flex; align-items: center; margin-bottom: 1.5em;'><span style='width: 1em; height: 1em; background: #000000; margin-right: 0.75em;'></span><h4 style='margin: 0; font-size: 1.3em; font-weight: 900; color: #000000;'>REFERENCES</h4></section><section style='font-size: 1em; line-height: 1.6; color: #000000; font-weight: 600;'>{{links}}</section></section>"
  },
  "blocks": {
    "paragraph": "<p style='font-size: 1em; color: #000000; margin: 0 0 1.5em 0; line-height: 1.7; font-family: inherit; text-align: left; font-weight: 500;'>{{text}}</p>",
    "header": {
      "h1": "<section style='margin: 3em 0 2.5em 0; text-align: center;'><section style='display: inline-block; background: #FF6B35; border: 6px solid #000000; box-shadow: 12px 12px 0px #000000;'><h1 style='font-size: 2.8em; font-weight: 900; line-height: 1.2; color: #000000; font-family: inherit; margin: 0; padding: 0.4em 0.8em; background: #FFFF00; border: 3px solid #000000;'>{{text}}</h1></section></section>",
      "h2": "<section style='margin: 2.5em 0 1.8em 0;'><section style='display: flex; align-items: center;'><span style='width: 0.8em; height: 3em; background: #FF6B35; border: 3px solid #000000; margin-right: 1em;'></span><h2 style='font-size: 2em; font-weight: 900; line-height: 1.3; color: #000000; font-family: inherit; margin: 0; background: #FFFFFF; padding: 0.3em 0.6em; border: 3px solid #000000; box-shadow: 5px 5px 0px #FF6B35;'>{{text}}</h2></section></section>",
      "h3": "<section style='margin: 2em 0 1.2em 0;'><section style='display: flex; align-items: center;'><section style='margin-right: 1em; display: flex; flex-direction: column; align-items: center;'><span style='width: 1.2em; height: 1.2em; background: #000000; border: 2px solid #FF6B35;'></span><span style='width: 0.3em; height: 1.5em; background: #FF6B35; border: 1px solid #000000;'></span></section><h3 style='font-size: 1.6em; font-weight: 800; line-height: 1.4; color: #FF4500; font-family: inherit; margin: 0; text-transform: uppercase;'>{{text}}</h3></section></section>"
    },
    "quote": {
      "template": "<section style='margin: 2.5em 0;'><section style='padding: 2em; background: #000000; border: 4px solid #FF6B35; font-family: inherit; box-shadow: 8px 8px 0px #FF6B35;'><section style='display: flex; align-items: flex-start;'><span style='font-size: 4em; color: #FF6B35; font-family: serif; line-height: 0.8; margin-right: 0.3em; font-weight: 900;'>\"</span><section style='flex: 1;'><p style='margin: 0 0 1em 0; color: #FFFFFF; font-style: italic; font-size: 1.2em; line-height: 1.6; font-weight: 600;'>{{text}}</p>{{?caption}}</section></section></section></section>",
      "optional": {
        "caption": "<p style='text-align: right; font-size: 1em; color: #FF6B35; font-style: normal; margin: 0.8em 0 0 0; font-weight: 800; text-transform: uppercase;'>— {{value}}</p>"
      }
    },
    "delimiter": "<section style='display: flex; align-items: center; justify-content: center; margin: 3em 0;'><section style='display: flex; align-items: center;'><span style='width: 1.2em; height: 1.2em; background: #FF6B35; border: 3px solid #000000; margin: 0 1em;'></span><span style='width: 0.8em; height: 0.8em; background: #000000; border: 2px solid #FF6B35; margin: 0 0.8em;'></span><span style='width: 1.2em; height: 1.2em; background: #FF6B35; border: 3px solid #000000; margin: 0 1em;'></span></section></section>",
    "image": {
      "template": "<section style='margin: 3em 0; text-align: center;'><section style='display: inline-block; border: 6px solid #000000; box-shadow: 10px 10px 0px #FF6B35; background: #FFFFFF; padding: 1em;'><img src='{{url}}' alt='{{alt}}' style='max-width: 100%; display: block; border: 3px solid #000000;' /></section>{{?caption}}</section>",
      "optional": {
        "caption": "<p style='margin: 1.5em 0 0 0; font-size: 1em; color: #000000; font-style: normal; line-height: 1.5; text-align: center; background: #FFFF00; padding: 0.5em 1em; border: 3px solid #000000; display: inline-block; font-weight: 700; text-transform: uppercase;'>{{value}}</p>"
      }
    },
    "code": "<section style='margin: 2.5em 0; background: #000000; border: 5px solid #FF6B35; box-shadow: 8px 8px 0px #FF6B35;'><section style='background: #FF6B35; padding: 1em 1.5em; border-bottom: 3px solid #000000; display: flex; align-items: center; justify-content: space-between;'><section style='display: flex; align-items: center;'><span style='width: 1em; height: 1em; background: #000000; margin-right: 0.8em;'></span><span style='width: 1em; height: 1em; background: #FFFF00; border: 2px solid #000000; margin-right: 0.8em;'></span><span style='width: 1em; height: 1em; background: #FFFFFF; border: 2px solid #000000;'></span></section><span style='font-size: 1em; color: #000000; font-weight: 900; text-transform: uppercase;'>CODE</span></section><section style='padding: 2em; font-family: \"Courier New\", monospace; font-size: 1em; line-height: 1.5; color: #00FF00; background: #000000; overflow-x: auto; font-weight: bold;'><span style='font-family: inherit; background: transparent; color: inherit; font-weight: 700;'>{{code}}</span></section></section>",
    "list": {
      "ordered": {
        "wrapper": "<ol style='margin: 2em 0; padding-left: 0; list-style: none;'>{{items}}</ol>",
        "item": "<li style='padding: 0.8em 0; line-height: 1.6; color: #000000; display: flex; align-items: flex-start; font-weight: 600;'><section style='margin-right: 1.5em; margin-top: 0.1em; flex-shrink: 0;'><span style='width: 2em; height: 2em; background: #FF6B35; border: 4px solid #000000; display: flex; align-items: center; justify-content: center; font-size: 1em; font-weight: 900; color: #000000; box-shadow: 4px 4px 0px #000000;'>{{index}}</span></section><span style='flex: 1; margin-top: 0.2em;'>{{content}}</span></li>"
      },
      "unordered": {
        "wrapper": "<ul style='margin: 2em 0; padding-left: 0; list-style: none;'>{{items}}</ul>",
        "item": "<li style='padding: 0.8em 0; line-height: 1.6; color: #000000; display: flex; align-items: flex-start; font-weight: 600;'><section style='margin-right: 1.5em; margin-top: 0.5em; flex-shrink: 0;'><span style='width: 1em; height: 1em; background: #FF6B35; border: 3px solid #000000;'></span></section><span style='flex: 1;'>{{content}}</span></li>"
      },
      "checklist": {
        "wrapper": "<ul style='margin: 2em 0; padding-left: 0; list-style: none;'>{{items}}</ul>",
        "item": "<li style='padding: 1em 0; line-height: 1.6; display: flex; align-items: flex-start; color: #000000; font-weight: 600;'><section style='margin-right: 1.2em; margin-top: 0.1em; width: 1.5em; height: 1.5em; border: 4px solid #000000; display: flex; align-items: center; justify-content: center; flex-shrink: 0; background: #FFFFFF; box-shadow: 3px 3px 0px #FF6B35;'>{{checkbox}}</section><span style='flex: 1; margin-top: 0.1em;'>{{content}}</span></li>",
        "checked": "<span style='color: #FF6B35; font-weight: 900; font-size: 1.2em;'>✓</span>",
        "unchecked": ""
      }
    },
    "raw": "{{html}}"
  }
}