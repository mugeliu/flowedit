{
  "id": "literary-green",
  "name": "90年代复古主题",
  "description": "怀旧复古风格，重现90年代经典视觉设计，充满时代感与个性",
  "category": "复古风格",
  "author": "FlowEdit Team",
  "version": "3.0.0",
  "inlineStyles": {
    "strong": "font-weight: 700; color: #ff6b35; background: #000000; padding: 0.15em 0.4em; font-family: 'Courier New', monospace; text-transform: uppercase; letter-spacing: 0.05em;",
    "b": "font-weight: 700; color: #ff6b35; background: #000000; padding: 0.15em 0.4em; font-family: 'Courier New', monospace; text-transform: uppercase; letter-spacing: 0.05em;",
    "em": "font-style: italic; color: #ff1493; font-weight: 600; background: #ffff00; padding: 0.1em 0.3em; text-decoration: underline; text-decoration-color: #ff1493;",
    "i": "font-style: italic; color: #ff1493; font-weight: 600; background: #ffff00; padding: 0.1em 0.3em; text-decoration: underline; text-decoration-color: #ff1493;",
    "u": "text-decoration: underline; text-decoration-color: #00ffff; text-decoration-thickness: 0.15em; text-underline-offset: 0.2em; color: #8a2be2; font-weight: 600;",
    "code": "font-family: 'Courier New', monospace; font-size: 0.9em; background: #000000; color: #00ff00; padding: 0.3em 0.6em; border: 2px solid #ff6b35; font-weight: 600; text-transform: uppercase; letter-spacing: 0.05em;",
    "mark": "background: #ff1493; color: #ffffff; padding: 0.2em 0.4em; font-weight: 700; text-shadow: 1px 1px 0px #000000; text-transform: uppercase;",
    "a": "color: #00ffff; text-decoration: underline; font-weight: 600; text-decoration-color: #ff6b35; text-decoration-thickness: 0.1em; background: #8a2be2; padding: 0.1em 0.2em;",
    "sup": "color: #ff6b35; font-size: 0.7em; font-weight: 700; vertical-align: super; background: #000000; padding: 0.1em 0.2em;"
  },
  "globalStyles": {
    "container": "<section style='max-width: 100%; margin: 0 auto; padding: 1.5em; font-size: 16px; line-height: 1.55; color: #000000; font-family: \"Courier New\", monospace; background: #ffffff; border: 3px solid #ff6b35; background: linear-gradient(45deg, #ffffff 25%, #f0f0f0 25%, #f0f0f0 50%, #ffffff 50%, #ffffff 75%, #f0f0f0 75%); background-size: 15px 15px; box-sizing: border-box;'>{{content}}</section>",
    "references": "<section style='margin-top: 2.5em; padding: 1.5em; background: #ffff00; border: 2px solid #ff1493; font-family: inherit; box-shadow: 3px 3px 0px #000000;'><section style='display: flex; align-items: center; margin-bottom: 1.2em; background: #ff1493; padding: 0.4em; margin: -0.4em -0.4em 0.8em -0.4em;'><span style='color: #ffff00; font-size: 1.1em; margin-right: 0.4em; font-weight: 700;'>►</span><h4 style='margin: 0; font-size: 1em; font-weight: 700; color: #ffffff; text-transform: uppercase; letter-spacing: 0.08em;'>REFERENCES</h4></section><section style='font-size: 0.85em; line-height: 1.4; color: #000000; font-weight: 600;'>{{links}}</section></section>"
  },
  "blocks": {
    "paragraph": "<p style='font-size: 1em; color: #000000; margin: 0 0 1.3em 0; line-height: 1.65; font-family: inherit; text-align: left; font-weight: 500;'>{{text}}</p>",
    "header": {
      "h1": "<section style='margin: 1.8em 0 1.4em 0; text-align: center;'><section style='display: inline-block; background: #ff1493; border: 2.5px solid #000000; box-shadow: 3px 3px 0px #ffff00;'><h1 style='font-size: 1.5em; font-weight: 900; line-height: 1.1; color: #ffffff; font-family: inherit; margin: 0; padding: 0.6em 1.2em; background: linear-gradient(90deg, #ff1493 0%, #8a2be2 50%, #ff1493 100%); text-transform: uppercase; letter-spacing: 0.06em; text-shadow: 1.5px 1.5px 0px #000000;'>{{text}}</h1></section></section>",
      "h2": "<section style='margin: 1.5em 0 1.1em 0;'><section style='background: #00ffff; border: 1.5px solid #000000; padding: 0.7em 1.2em; box-shadow: 2.5px 2.5px 0px #ff6b35;'><h2 style='font-size: 1.3em; font-weight: 800; line-height: 1.2; color: #000000; font-family: inherit; margin: 0; text-transform: uppercase; letter-spacing: 0.03em;'>{{text}}</h2></section></section>",
      "h3": "<section style='margin: 1.2em 0 0.8em 0;'><section style='background: #ffff00; border-left: 5px solid #ff1493; border-top: 1.5px solid #000000; border-right: 1.5px solid #000000; border-bottom: 1.5px solid #000000; padding: 0.7em 1.2em;'><h3 style='font-size: 1.1em; font-weight: 700; line-height: 1.25; color: #8a2be2; font-family: inherit; margin: 0; text-transform: uppercase; letter-spacing: 0.015em;'>{{text}}</h3></section></section>"
    },
    "quote": {
      "template": "<section style='margin: 1.5em 0;'><section style='padding: 1.2em; background: #8a2be2; border: 2.5px solid #ffff00; font-family: inherit; box-shadow: 3px 3px 0px #000000;'><section style='background: #000000; padding: 1em; border: 1.5px solid #00ffff;'><p style='margin: 0 0 0.6em 0; color: #ffffff; font-style: italic; font-size: 1em; line-height: 1.45; font-weight: 600; text-align: center; text-shadow: 1px 1px 0px #ff1493;'>{{text}}</p>{{?caption}}</section></section></section>",
      "optional": {
        "caption": "<p style='text-align: center; font-size: 0.85em; color: #ffff00; font-style: normal; margin: 0.6em 0 0 0; font-weight: 700; text-transform: uppercase; letter-spacing: 0.06em;'>— {{value}} —</p>"
      }
    },
    "delimiter": "<section style='display: flex; align-items: center; justify-content: center; margin: 1.8em 0; background: #000000; padding: 0.6em; border: 1.5px solid #ff6b35;'><span style='color: #ff1493; font-size: 1.1em; margin: 0 0.6em; font-weight: 900;'>●</span><span style='color: #00ffff; font-size: 1.5em; margin: 0 0.6em; font-weight: 900;'>◆</span><span style='color: #ffff00; font-size: 1.3em; margin: 0 0.6em; font-weight: 900;'>▲</span><span style='color: #00ffff; font-size: 1.5em; margin: 0 0.6em; font-weight: 900;'>◆</span><span style='color: #ff1493; font-size: 1.1em; margin: 0 0.6em; font-weight: 900;'>●</span></section>",
    "image": {
      "template": "<section style='margin: 1.8em 0; text-align: center;'><section style='display: inline-block; background: #ffff00; border: 2.5px solid #000000; box-shadow: 3px 3px 0px #ff1493; padding: 0.5em;'><section style='border: 1.5px solid #00ffff;'><img src='{{url}}' alt='{{alt}}' style='max-width: 100%; display: block;' /></section></section>{{?caption}}</section>",
      "optional": {
        "caption": "<p style='margin: 1em 0 0 0; font-size: 0.85em; color: #000000; font-style: normal; line-height: 1.35; text-align: center; background: #ff6b35; padding: 0.5em 0.9em; border: 1.5px solid #000000; display: inline-block; font-weight: 700; text-transform: uppercase; color: #ffffff; text-shadow: 1px 1px 0px #000000;'>{{value}}</p>"
      }
    },
    "code": "<section style='margin: 1.5em 0; background: #000000; border: 2.5px solid #00ffff; box-shadow: 3px 3px 0px #ff1493;'><section style='background: #ff6b35; padding: 0.6em 1em; border-bottom: 1.5px solid #ffff00; display: flex; align-items: center; justify-content: space-between;'><section style='display: flex; align-items: center;'><span style='width: 0.7em; height: 0.7em; background: #ff1493; margin-right: 0.5em; border: 1.5px solid #ffffff;'></span><span style='width: 0.7em; height: 0.7em; background: #ffff00; margin-right: 0.5em; border: 1.5px solid #000000;'></span><span style='width: 0.7em; height: 0.7em; background: #00ffff; border: 1.5px solid #000000;'></span></section><span style='font-size: 0.85em; color: #ffffff; font-weight: 900; text-transform: uppercase; letter-spacing: 0.06em; text-shadow: 1px 1px 0px #000000;'>CODE</span></section><section style='padding: 1.2em; font-family: \"Courier New\", monospace; font-size: 0.85em; line-height: 1.35; color: #00ff00; background: #000000; overflow-x: auto; font-weight: 600;'><span style='font-family: inherit; background: transparent; color: inherit; font-weight: 700;'>{{code}}</span></section></section>",
    "list": {
      "ordered": {
        "wrapper": "<ol style='margin: 1.2em 0; padding-left: 0; list-style: none; background: #f0f0f0; border: 1.5px solid #000000; padding: 1em; box-shadow: 2.5px 2.5px 0px #8a2be2;'>{{items}}</ol>",
        "item": "<li style='padding: 0.6em 0; line-height: 1.45; color: #000000; display: flex; align-items: flex-start; font-weight: 600; border-bottom: 1.5px dotted #ff1493; margin-bottom: 0.3em;'><section style='margin-right: 1em; margin-top: 0.08em; flex-shrink: 0;'><span style='width: 1.5em; height: 1.5em; background: #ff1493; border: 1.5px solid #000000; display: flex; align-items: center; justify-content: center; font-size: 0.85em; font-weight: 900; color: #ffffff; box-shadow: 1.5px 1.5px 0px #ffff00; text-shadow: 1px 1px 0px #000000;'>{{index}}</span></section><span style='flex: 1; margin-top: 0.2em;'>{{content}}</span></li>"
      },
      "unordered": {
        "wrapper": "<ul style='margin: 1.2em 0; padding-left: 0; list-style: none; background: #f0f0f0; border: 1.5px solid #000000; padding: 1em; box-shadow: 2.5px 2.5px 0px #00ffff;'>{{items}}</ul>",
        "item": "<li style='padding: 0.6em 0; line-height: 1.45; color: #000000; display: flex; align-items: flex-start; font-weight: 600; border-bottom: 1.5px dotted #00ffff; margin-bottom: 0.3em;'><section style='margin-right: 1em; margin-top: 0.35em; flex-shrink: 0;'><span style='width: 0.8em; height: 0.8em; background: #00ffff; border: 1.5px solid #000000; display: inline-block; box-shadow: 1.5px 1.5px 0px #ff6b35;'></span></section><span style='flex: 1;'>{{content}}</span></li>"
      },
      "checklist": {
        "wrapper": "<ul style='margin: 1.2em 0; padding-left: 0; list-style: none; background: #f0f0f0; border: 1.5px solid #000000; padding: 1em; box-shadow: 2.5px 2.5px 0px #ffff00;'>{{items}}</ul>",
        "item": "<li style='padding: 0.6em 0; line-height: 1.45; display: flex; align-items: flex-start; color: #000000; font-weight: 600; border-bottom: 1.5px dotted #ff6b35; margin-bottom: 0.3em;'><section style='margin-right: 0.8em; margin-top: 0.08em; width: 1.2em; height: 1.2em; border: 1.5px solid #000000; display: flex; align-items: center; justify-content: center; flex-shrink: 0; background: #ffffff; box-shadow: 1.5px 1.5px 0px #ff1493;'>{{checkbox}}</section><span style='flex: 1; margin-top: 0.12em;'>{{content}}</span></li>",
        "checked": "<span style='color: #ff1493; font-weight: 900; font-size: 1em; text-shadow: 1px 1px 0px #ffff00;'>✓</span>",
        "unchecked": ""
      }
    },
    "raw": "{{html}}"
  }
}