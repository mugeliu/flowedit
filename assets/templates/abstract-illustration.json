{
  "id": "abstract-illustration",
  "name": "抽象插画主题",
  "description": "现代抽象艺术风格，融合几何形状与流动线条，创造独特的视觉体验",
  "category": "抽象艺术",
  "author": "FlowEdit Team",
  "version": "1.0.0",
  "inlineStyles": {
    "strong": "font-weight: 700; color: #e74c3c; background: linear-gradient(135deg, #fff5f5 0%, #ffeaea 100%); padding: 0.2em 0.4em; border-radius: 20px; position: relative; box-shadow: 0 2px 8px rgba(231, 76, 60, 0.2);",
    "b": "font-weight: 700; color: #e74c3c; background: linear-gradient(135deg, #fff5f5 0%, #ffeaea 100%); padding: 0.2em 0.4em; border-radius: 20px; position: relative; box-shadow: 0 2px 8px rgba(231, 76, 60, 0.2);",
    "em": "font-style: italic; color: #8e44ad; font-weight: 600; background: linear-gradient(45deg, #f8f5ff 0%, #f0e8ff 100%); padding: 0.15em 0.3em; border-radius: 15px; box-shadow: 0 1px 4px rgba(142, 68, 173, 0.15);",
    "i": "font-style: italic; color: #8e44ad; font-weight: 600; background: linear-gradient(45deg, #f8f5ff 0%, #f0e8ff 100%); padding: 0.15em 0.3em; border-radius: 15px; box-shadow: 0 1px 4px rgba(142, 68, 173, 0.15);",
    "u": "text-decoration: none; background: linear-gradient(90deg, #3498db 0%, #9b59b6 50%, #e74c3c 100%); background-size: 100% 0.15em; background-position: 0 85%; background-repeat: no-repeat; padding-bottom: 0.1em; color: #2c3e50;",
    "code": "font-family: 'SF Mono', 'Monaco', 'Menlo', monospace; font-size: 0.9em; background: linear-gradient(135deg, #2c3e50 0%, #34495e 100%); color: #ecf0f1; padding: 0.3em 0.6em; border-radius: 25px; font-weight: 600; box-shadow: 0 3px 12px rgba(44, 62, 80, 0.3), inset 0 1px 0 rgba(255,255,255,0.1);",
    "mark": "background: linear-gradient(135deg, #f1c40f 0%, #f39c12 100%); color: #2c3e50; padding: 0.2em 0.4em; border-radius: 20px; font-weight: 600; box-shadow: 0 2px 8px rgba(241, 196, 15, 0.3);",
    "a": "color: #3498db; text-decoration: none; font-weight: 600; background: linear-gradient(90deg, #3498db 0%, #3498db 100%); background-size: 0 0.12em; background-position: 0 90%; background-repeat: no-repeat; border-radius: 15px; padding: 0.1em 0.2em;",
    "sup": "color: #95a5a6; font-size: 0.75em; font-weight: 600; vertical-align: super; background: linear-gradient(135deg, #ecf0f1 0%, #bdc3c7 100%); padding: 0.1em 0.3em; border-radius: 10px;"
  },
  "globalStyles": {
    "container": "<section style='max-width: 100%; margin: 0 auto; padding: 1.6em 1.2em; font-size: 16px; line-height: 1.7; color: #2c3e50; font-family: -apple-system, BlinkMacSystemFont, \"Segoe UI\", Roboto, \"Helvetica Neue\", Arial, sans-serif; background: linear-gradient(135deg, #ffffff 0%, #f8f9fa 100%); position: relative; overflow: hidden;'>{{content}}</section>",
    "references": "<section style='margin-top: 2.3em; padding: 1.6em; background: linear-gradient(135deg, #e8f5e8 0%, #f0f8f0 100%); border-radius: 25px; font-family: inherit; position: relative; overflow: hidden; box-shadow: 0 6px 24px rgba(46, 204, 113, 0.08);'><section style='position: absolute; top: -15px; left: -15px; width: 60px; height: 60px; background: linear-gradient(45deg, #2ecc71 0%, #27ae60 100%); border-radius: 50%; opacity: 0.08;'></section><section style='position: absolute; bottom: -20px; right: -20px; width: 70px; height: 70px; background: linear-gradient(135deg, #3498db 0%, #2980b9 100%); border-radius: 30% 70% 70% 30% / 30% 30% 70% 70%; opacity: 0.06;'></section><section style='display: flex; align-items: center; margin-bottom: 1.1em; position: relative; z-index: 1;'><span style='width: 0.8em; height: 0.8em; background: linear-gradient(45deg, #2ecc71 0%, #27ae60 100%); border-radius: 50%; margin-right: 0.8em; box-shadow: 0 3px 8px rgba(46, 204, 113, 0.25);'></span><h4 style='margin: 0; font-size: 1.1em; font-weight: 700; color: #27ae60;'>参考资料</h4></section><section style='font-size: 0.9em; line-height: 1.6; color: #2c3e50; position: relative; z-index: 1;'>{{links}}</section></section>"
  },
  "blocks": {
    "paragraph": "<p style='font-size: 1em; color: #2c3e50; margin: 0 0 1.3em 0; line-height: 1.8; font-family: inherit; text-align: left; font-weight: 400; position: relative;'>{{text}}</p>",
    "header": {
      "h1": "<section style='margin: 1.8em 0 1.4em 0; text-align: center; position: relative;'><section style='position: absolute; top: -15px; left: 50%; width: 60px; height: 60px; background: linear-gradient(45deg, #e74c3c 0%, #c0392b 100%); border-radius: 50% 30% 50% 30%; opacity: 0.08; margin-left: -30px;'></section><h1 style='font-size: 1.4em; font-weight: 800; line-height: 1.1; color: #2c3e50; font-family: inherit; margin: 0; padding: 0.8em 1.4em; background: linear-gradient(135deg, #ffffff 0%, #f8f9fa 100%); border-radius: 35px; box-shadow: 0 8px 30px rgba(0,0,0,0.08); position: relative; z-index: 1; overflow: hidden;'><span style='position: absolute; top: 6px; right: 6px; width: 25px; height: 25px; background: linear-gradient(45deg, #3498db 0%, #2980b9 100%); border-radius: 50% 30% 50% 30%; opacity: 0.15;'></span>{{text}}</h1></section>",
      "h2": "<section style='margin: 1.4em 0 1em 0; position: relative;'><section style='position: absolute; left: -10px; top: 50%; width: 32px; height: 32px; background: linear-gradient(135deg, #9b59b6 0%, #8e44ad 100%); border-radius: 30% 70% 70% 30% / 30% 30% 70% 70%; opacity: 0.12; margin-top: -16px;'></section><h2 style='font-size: 1.2em; font-weight: 700; line-height: 1.2; color: #2c3e50; font-family: inherit; margin: 0; padding: 0.7em 1.2em 0.7em 1.8em; background: linear-gradient(135deg, #f8f9fa 0%, #e9ecef 100%); border-radius: 0 30px 30px 0; position: relative; z-index: 1; box-shadow: 0 4px 15px rgba(0,0,0,0.06);'>{{text}}</h2></section>",
      "h3": "<section style='margin: 1.2em 0 0.7em 0; position: relative;'><section style='position: absolute; right: -6px; top: -6px; width: 16px; height: 16px; background: linear-gradient(45deg, #f39c12 0%, #e67e22 100%); border-radius: 50% 30% 50% 30%; opacity: 0.25;'></section><h3 style='font-size: 1.05em; font-weight: 650; line-height: 1.25; color: #34495e; font-family: inherit; margin: 0; padding: 0.6em 0.9em; background: linear-gradient(90deg, #ffffff 0%, #f8f9fa 100%); border-left: 4px solid transparent; border-image: linear-gradient(135deg, #3498db 0%, #9b59b6 100%) 1; border-radius: 0 16px 16px 0; position: relative; z-index: 1;'>{{text}}</h3></section>"
    },
    "quote": {
      "template": "<section style='margin: 1.4em 0; position: relative;'><section style='position: absolute; top: -12px; left: -12px; width: 50px; height: 50px; background: linear-gradient(135deg, #e74c3c 0%, #c0392b 100%); border-radius: 50% 30% 50% 30%; opacity: 0.06;'></section><section style='position: absolute; bottom: -10px; right: -10px; width: 38px; height: 38px; background: linear-gradient(45deg, #9b59b6 0%, #8e44ad 100%); border-radius: 30% 70% 70% 30% / 30% 30% 70% 70%; opacity: 0.08;'></section><section style='padding: 1.3em; background: linear-gradient(135deg, #f8f9fa 0%, #e9ecef 100%); border-radius: 25px; font-family: inherit; position: relative; z-index: 1; overflow: hidden; box-shadow: 0 8px 25px rgba(0,0,0,0.04);'><section style='position: absolute; top: 10px; left: 12px; width: 5px; height: 5px; background: linear-gradient(45deg, #3498db 0%, #2980b9 100%); border-radius: 50%;'></section><p style='margin: 0 0 0.7em 0; color: #2c3e50; font-style: italic; font-size: 0.95em; line-height: 1.65; font-weight: 500; text-align: center; position: relative;'>{{text}}</p>{{?caption}}</section></section>",
      "optional": {
        "caption": "<p style='text-align: center; font-size: 0.85em; color: #7f8c8d; font-style: normal; margin: 0.6em 0 0 0; font-weight: 600; position: relative;'>— {{value}}</p>"
      }
    },
    "delimiter": "<section style='display: flex; align-items: center; justify-content: center; margin: 1.8em 0; position: relative;'><section style='position: absolute; left: 25%; width: 12px; height: 12px; background: linear-gradient(45deg, #e74c3c 0%, #c0392b 100%); border-radius: 50% 30% 50% 30%; opacity: 0.25;'></section><section style='position: absolute; right: 30%; width: 10px; height: 10px; background: linear-gradient(135deg, #3498db 0%, #2980b9 100%); border-radius: 30% 70% 70% 30% / 30% 30% 70% 70%; opacity: 0.3;'></section><span style='width: 2.5em; height: 0.12em; background: linear-gradient(90deg, #bdc3c7 0%, #95a5a6 50%, #bdc3c7 100%); border-radius: 0.06em; margin: 0 0.6em;'></span><span style='width: 0.8em; height: 0.8em; background: linear-gradient(135deg, #f39c12 0%, #e67e22 100%); border-radius: 50% 30% 50% 30%; margin: 0 0.3em; box-shadow: 0 2px 8px rgba(243, 156, 18, 0.25);'></span><span style='width: 2.5em; height: 0.12em; background: linear-gradient(90deg, #bdc3c7 0%, #95a5a6 50%, #bdc3c7 100%); border-radius: 0.06em; margin: 0 0.6em;'></span></section>",
    "image": {
      "template": "<section style='margin: 1.6em 0; text-align: center; position: relative;'><section style='position: absolute; top: -14px; left: 30%; width: 28px; height: 28px; background: linear-gradient(45deg, #2ecc71 0%, #27ae60 100%); border-radius: 50% 30% 50% 30%; opacity: 0.08;'></section><section style='display: inline-block; border-radius: 20px; overflow: hidden; box-shadow: 0 10px 35px rgba(0, 0, 0, 0.12); background: linear-gradient(135deg, #ffffff 0%, #f8f9fa 100%); padding: 0.5em; position: relative; z-index: 1;'><section style='border-radius: 12px; overflow: hidden; position: relative;'><img src='{{url}}' alt='{{alt}}' style='max-width: 100%; display: block;' /></section></section>{{?caption}}</section>",
      "optional": {
        "caption": "<p style='margin: 0.9em 0 0 0; font-size: 0.8em; color: #7f8c8d; font-style: italic; line-height: 1.45; text-align: center; background: linear-gradient(135deg, #ecf0f1 0%, #bdc3c7 100%); padding: 0.4em 0.8em; border-radius: 16px; display: inline-block; font-weight: 500;'>{{value}}</p>"
      }
    },
    "code": "<section style='margin: 1.4em 0; position: relative; overflow: hidden;'><section style='position: absolute; top: -12px; right: -12px; width: 38px; height: 38px; background: linear-gradient(135deg, #34495e 0%, #2c3e50 100%); border-radius: 30% 70% 70% 30% / 30% 30% 70% 70%; opacity: 0.08;'></section><section style='background: linear-gradient(135deg, #2c3e50 0%, #34495e 100%); border-radius: 16px; overflow: hidden; box-shadow: 0 8px 30px rgba(44, 62, 80, 0.25); position: relative; z-index: 1;'><section style='background: linear-gradient(90deg, #34495e 0%, #2c3e50 100%); padding: 0.5em 1em; display: flex; align-items: center; justify-content: space-between;'><section style='display: flex; align-items: center;'><span style='width: 0.6em; height: 0.6em; background: linear-gradient(45deg, #e74c3c 0%, #c0392b 100%); border-radius: 50%; margin-right: 0.5em; box-shadow: 0 1.5px 4px rgba(231, 76, 60, 0.3);'></span><span style='width: 0.6em; height: 0.6em; background: linear-gradient(45deg, #f39c12 0%, #e67e22 100%); border-radius: 50%; margin-right: 0.5em; box-shadow: 0 1.5px 4px rgba(243, 156, 18, 0.3);'></span><span style='width: 0.6em; height: 0.6em; background: linear-gradient(45deg, #2ecc71 0%, #27ae60 100%); border-radius: 50%; box-shadow: 0 1.5px 4px rgba(46, 204, 113, 0.3);'></span></section><span style='font-size: 0.7em; color: #bdc3c7; font-weight: 600; text-transform: uppercase; letter-spacing: 0.06em;'>代码</span></section><section style='padding: 1em; font-family: \"SF Mono\", \"Monaco\", \"Menlo\", monospace; font-size: 0.85em; line-height: 1.45; color: #ecf0f1; background: linear-gradient(135deg, #2c3e50 0%, #34495e 100%); overflow-x: auto;'><span style='font-family: inherit; background: transparent; color: inherit; font-weight: 500;'>{{code}}</span></section></section></section>",
    "list": {
      "ordered": {
        "wrapper": "<ol style='margin: 1.2em 0; padding-left: 0; list-style: none; position: relative;'>{{items}}</ol>",
        "item": "<li style='padding: 0.6em 0; line-height: 1.65; color: #2c3e50; display: flex; align-items: flex-start; position: relative;'><section style='margin-right: 1em; margin-top: 0.12em; flex-shrink: 0; position: relative;'><section style='position: absolute; top: -5px; left: -5px; width: 1.8em; height: 1.8em; background: linear-gradient(135deg, #3498db 0%, #2980b9 100%); border-radius: 50% 30% 50% 30%; opacity: 0.08;'></section><span style='width: 1.3em; height: 1.3em; background: linear-gradient(135deg, #3498db 0%, #2980b9 100%); border-radius: 50%; display: flex; align-items: center; justify-content: center; font-size: 0.75em; font-weight: 700; color: white; box-shadow: 0 3px 12px rgba(52, 152, 219, 0.25); position: relative; z-index: 1;'>{{index}}</span></section><span style='flex: 1; margin-top: 0.12em;'>{{content}}</span></li>"
      },
      "unordered": {
        "wrapper": "<ul style='margin: 1.2em 0; padding-left: 0; list-style: none; position: relative;'>{{items}}</ul>",
        "item": "<li style='padding: 0.6em 0; line-height: 1.65; color: #2c3e50; display: flex; align-items: flex-start; position: relative;'><section style='margin-right: 1em; margin-top: 0.35em; flex-shrink: 0; position: relative;'><section style='position: absolute; top: -3px; left: -3px; width: 1em; height: 1em; background: linear-gradient(45deg, #e74c3c 0%, #c0392b 100%); border-radius: 50% 30% 50% 30%; opacity: 0.12;'></section><span style='width: 0.4em; height: 0.4em; background: linear-gradient(135deg, #e74c3c 0%, #c0392b 100%); border-radius: 50% 30% 50% 30%; display: inline-block; box-shadow: 0 1.5px 6px rgba(231, 76, 60, 0.25); position: relative; z-index: 1;'></span></section><span style='flex: 1;'>{{content}}</span></li>"
      },
      "checklist": {
        "wrapper": "<ul style='margin: 1.2em 0; padding-left: 0; list-style: none; position: relative;'>{{items}}</ul>",
        "item": "<li style='padding: 0.6em 0; line-height: 1.65; display: flex; align-items: flex-start; color: #2c3e50; position: relative;'><section style='margin-right: 0.8em; margin-top: 0.1em; width: 1em; height: 1em; border-radius: 5px; display: flex; align-items: center; justify-content: center; flex-shrink: 0; background: linear-gradient(135deg, #ecf0f1 0%, #bdc3c7 100%); box-shadow: 0 2px 8px rgba(0,0,0,0.08); position: relative;'><section style='position: absolute; top: -2px; right: -2px; width: 5px; height: 5px; background: linear-gradient(45deg, #f39c12 0%, #e67e22 100%); border-radius: 50%; opacity: 0.5;'></section>{{checkbox}}</section><span style='flex: 1; margin-top: 0.03em;'>{{content}}</span></li>",
        "checked": "<span style='color: #27ae60; font-weight: 800; font-size: 0.75em;'>✓</span>",
        "unchecked": ""
      }
    },
    "raw": "{{html}}"
  }
}