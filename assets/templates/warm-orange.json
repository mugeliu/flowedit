{
  "id": "warm-orange",
  "name": "新粗野橙潮主题",
  "description": "大胆几何与强烈对比的新粗野主义设计，极具视觉冲击力",
  "category": "新粗野主义",
  "author": "FlowEdit Team",
  "version": "2.0.0",
  "inlineStyles": {
    "strong": "font-weight: 900; color: #000000; background: #FF6B35; padding: 0.1em 0.25em; border: 1.5px solid #000000; border-radius: 0.15em;",
    "b": "font-weight: 900; color: #000000; background: #FF6B35; padding: 0.1em 0.25em; border: 1.5px solid #000000; border-radius: 0.15em;",
    "em": "font-style: italic; color: #FF4500; font-weight: 700; background: #FFFF00; padding: 0.05em 0.15em; border: 1px solid #000000; border-radius: 0.1em;",
    "i": "font-style: italic; color: #FF4500; font-weight: 700; background: #FFFF00; padding: 0.05em 0.15em; border: 1px solid #000000; border-radius: 0.1em;",
    "u": "text-decoration: none; color: #000000; background: #FF6B35; padding: 0.1em 0.15em; border-bottom: 3px solid #000000;",
    "code": "font-family: 'Courier New', monospace; font-size: 0.85em; background: #000000; color: #00FF00; padding: 0.25em 0.5em; border: 2px solid #FF6B35; font-weight: 700; border-radius: 0.2em;",
    "mark": "background: #FFFF00; color: #000000; padding: 0.15em 0.3em; border: 1.5px solid #000000; font-weight: 700; border-radius: 0.15em;",
    "a": "color: #FF4500; text-decoration: none; font-weight: 800; background: #FFFFFF; padding: 0.1em 0.15em; border: 1.5px solid #000000; box-shadow: 2px 2px 0px #000000; border-radius: 0.1em;",
    "sup": "color: #FF6B35; font-size: 0.7em; font-weight: 900; background: #000000; padding: 0.08em 0.15em; vertical-align: super; border-radius: 0.1em;"
  },
  "globalStyles": {
    "container": "<section style='max-width: 100%; margin: 0 auto; padding: 1.2em 0.8em; font-size: 15px; line-height: 1.65; color: #000000; font-family: -apple-system, BlinkMacSystemFont, \"Helvetica Neue\", \"PingFang SC\", \"Hiragino Sans GB\", Arial, sans-serif; background: #FFFFFF; border: 2px solid #000000; box-shadow: 4px 4px 0px #FF6B35; font-weight: 600; border-radius: 0.4em;'>{{content}}</section>",
    "references": "<section style='margin-top: 1.5em; padding: 1.2em; background: #FF6B35; border: 2px solid #000000; font-family: inherit; box-shadow: 3px 3px 0px #000000; border-radius: 0.4em;'><section style='display: flex; align-items: center; margin-bottom: 0.8em;'><span style='width: 0.6em; height: 0.6em; background: #000000; margin-right: 0.5em; border-radius: 0.1em;'></span><h4 style='margin: 0; font-size: 1em; font-weight: 900; color: #000000;'>参考资料</h4></section><section style='font-size: 0.85em; line-height: 1.5; color: #000000; font-weight: 600;'>{{links}}</section></section>"
  },
  "blocks": {
    "paragraph": "<p style='font-size: 1em; color: #000000; margin: 0 0 1.2em 0; line-height: 1.65; font-family: inherit; text-align: left; font-weight: 500;'>{{text}}</p>",
    "header": {
      "h1": "<section style='margin: 1.4em 0 1.1em 0; text-align: center;'><section style='display: inline-block; background: #FF6B35; border: 2.5px solid #000000; box-shadow: 4px 4px 0px #000000; border-radius: 0.3em;'><h1 style='font-size: 1.4em; font-weight: 900; line-height: 1.15; color: #000000; font-family: inherit; margin: 0; padding: 0.5em 0.8em; background: #FFFF00; border: 1.5px solid #000000; border-radius: 0.2em;'>{{text}}</h1></section></section>",
      "h2": "<section style='margin: 1.2em 0 0.9em 0;'><section style='display: flex; align-items: center;'><span style='width: 0.4em; height: 1.6em; background: #FF6B35; border: 1.5px solid #000000; margin-right: 0.5em; border-radius: 0.12em;'></span><h2 style='font-size: 1.2em; font-weight: 900; line-height: 1.2; color: #000000; font-family: inherit; margin: 0; background: #FFFFFF; padding: 0.4em 0.6em; border: 1.5px solid #000000; box-shadow: 2px 2px 0px #FF6B35; border-radius: 0.2em;'>{{text}}</h2></section></section>",
      "h3": "<section style='margin: 1.1em 0 0.7em 0;'><section style='display: flex; align-items: center;'><section style='margin-right: 0.5em; display: flex; flex-direction: column; align-items: center;'><span style='width: 0.7em; height: 0.7em; background: #000000; border: 1px solid #FF6B35; border-radius: 0.08em;'></span><span style='width: 0.15em; height: 0.8em; background: #FF6B35; border: 1px solid #000000; border-radius: 0.04em;'></span></section><h3 style='font-size: 1.05em; font-weight: 800; line-height: 1.25; color: #FF4500; font-family: inherit; margin: 0; text-transform: uppercase;'>{{text}}</h3></section></section>"
    },
    "quote": {
      "template": "<section style='margin: 1.2em 0;'><section style='padding: 1em; background: #000000; border: 1.5px solid #FF6B35; font-family: inherit; box-shadow: 3px 3px 0px #FF6B35; border-radius: 0.3em;'><section style='display: flex; align-items: flex-start;'><span style='font-size: 2em; color: #FF6B35; font-family: serif; line-height: 0.7; margin-right: 0.3em; font-weight: 900;'>\"</span><section style='flex: 1;'><p style='margin: 0 0 0.5em 0; color: #FFFFFF; font-style: italic; font-size: 0.95em; line-height: 1.45; font-weight: 600;'>{{text}}</p>{{?caption}}</section></section></section></section>",
      "optional": {
        "caption": "<p style='text-align: right; font-size: 0.8em; color: #FF6B35; font-style: normal; margin: 0.4em 0 0 0; font-weight: 800; text-transform: uppercase;'>— {{value}}</p>"
      }
    },
    "delimiter": "<section style='display: flex; align-items: center; justify-content: center; margin: 1.5em 0;'><section style='display: flex; align-items: center;'><span style='width: 0.7em; height: 0.7em; background: #FF6B35; border: 1.5px solid #000000; margin: 0 0.5em; border-radius: 0.08em;'></span><span style='width: 0.4em; height: 0.4em; background: #000000; border: 1px solid #FF6B35; margin: 0 0.4em; border-radius: 0.06em;'></span><span style='width: 0.7em; height: 0.7em; background: #FF6B35; border: 1.5px solid #000000; margin: 0 0.5em; border-radius: 0.08em;'></span></section></section>",
    "image": {
      "template": "<section style='margin: 1.5em 0; text-align: center;'><section style='display: inline-block; border: 2.5px solid #000000; box-shadow: 3px 3px 0px #FF6B35; background: #FFFFFF; padding: 0.5em; border-radius: 0.3em;'><img src='{{url}}' alt='{{alt}}' style='max-width: 100%; display: block; border: 1.5px solid #000000; border-radius: 0.2em;' /></section>{{?caption}}</section>",
      "optional": {
        "caption": "<p style='margin: 0.8em 0 0 0; font-size: 0.8em; color: #000000; font-style: normal; line-height: 1.35; text-align: center; background: #FFFF00; padding: 0.4em 0.7em; border: 1.5px solid #000000; display: inline-block; font-weight: 700; text-transform: uppercase; border-radius: 0.2em;'>{{value}}</p>"
      }
    },
    "code": "<section style='margin: 1.2em 0; background: #000000; border: 1.5px solid #FF6B35; box-shadow: 3px 3px 0px #FF6B35; border-radius: 0.3em; overflow: hidden;'><section style='background: #FF6B35; padding: 0.5em 0.8em; border-bottom: 1.5px solid #000000; display: flex; align-items: center; justify-content: space-between;'><section style='display: flex; align-items: center;'><span style='width: 0.5em; height: 0.5em; background: #000000; margin-right: 0.4em; border-radius: 0.06em;'></span><span style='width: 0.5em; height: 0.5em; background: #FFFF00; border: 1px solid #000000; margin-right: 0.4em; border-radius: 0.06em;'></span><span style='width: 0.5em; height: 0.5em; background: #FFFFFF; border: 1px solid #000000; border-radius: 0.06em;'></span></section><span style='font-size: 0.75em; color: #000000; font-weight: 900; text-transform: uppercase;'>代码</span></section><section style='padding: 1em 0.8em; font-family: \"Courier New\", monospace; font-size: 0.75em; line-height: 1.35; color: #00FF00; background: #000000; overflow-x: auto; font-weight: bold;'><span style='font-family: inherit; background: transparent; color: inherit; font-weight: 700;'>{{code}}</span></section></section>",
    "list": {
      "ordered": {
        "wrapper": "<ol style='margin: 1.1em 0; padding-left: 0; list-style: none;'>{{items}}</ol>",
        "item": "<li style='padding: 0.4em 0; line-height: 1.55; color: #000000; display: flex; align-items: flex-start; font-weight: 600; margin-bottom: 0.2em;'><section style='margin-right: 0.8em; margin-top: 0.08em; flex-shrink: 0;'><span style='width: 1.2em; height: 1.2em; background: #FF6B35; border: 1.5px solid #000000; display: flex; align-items: center; justify-content: center; font-size: 0.75em; font-weight: 900; color: #000000; box-shadow: 1.5px 1.5px 0px #000000; border-radius: 0.12em;'>{{index}}</span></section><span style='flex: 1; margin-top: 0.08em;'>{{content}}</span></li>"
      },
      "unordered": {
        "wrapper": "<ul style='margin: 1.1em 0; padding-left: 0; list-style: none;'>{{items}}</ul>",
        "item": "<li style='padding: 0.4em 0; line-height: 1.55; color: #000000; display: flex; align-items: flex-start; font-weight: 600; margin-bottom: 0.2em;'><section style='margin-right: 0.8em; margin-top: 0.3em; flex-shrink: 0;'><span style='width: 0.6em; height: 0.6em; background: #FF6B35; border: 1.5px solid #000000; border-radius: 0.08em;'></span></section><span style='flex: 1;'>{{content}}</span></li>"
      },
      "checklist": {
        "wrapper": "<ul style='margin: 1.1em 0; padding-left: 0; list-style: none;'>{{items}}</ul>",
        "item": "<li style='padding: 0.5em 0; line-height: 1.55; display: flex; align-items: flex-start; color: #000000; font-weight: 600; margin-bottom: 0.2em;'><section style='margin-right: 0.7em; margin-top: 0.08em; width: 0.9em; height: 0.9em; border: 1.5px solid #000000; display: flex; align-items: center; justify-content: center; flex-shrink: 0; background: #FFFFFF; box-shadow: 1.5px 1.5px 0px #FF6B35; border-radius: 0.08em;'>{{checkbox}}</section><span style='flex: 1; margin-top: 0.04em;'>{{content}}</span></li>",
        "checked": "<span style='color: #FF6B35; font-weight: 900; font-size: 0.85em;'>✓</span>",
        "unchecked": ""
      }
    },
    "raw": "{{html}}"
  }
}