      },
      "unordered": {
        "wrapper": "<ul style='margin: 2em 0.05em; padding-left: 0; list-style: none; background: #f8f4ee; background-image: radial-gradient(ellipse 55px 75px at 88% 12%, rgba(122, 79, 46, 0.045) 0%, transparent 42%); border: 2px solid #c49670; border-radius: 15px 17px 14px 16px; padding: 1.4em 1.5em 1.6em 1.7em; box-shadow: 0 4px 16px rgba(122, 79, 46, 0.08), inset 0 0.5px 0 rgba(255, 255, 255, 0.28); position: relative;'>{{items}}</ul>",
        "item": "<li style='padding: 0.9em 0 1.1em 0.02em; line-height: 1.75; color: #5a4930; display: flex; align-items: flex-start; position: relative; border-bottom: 1px dashed #c49670; margin-bottom: 0.6em;'><section style='margin-right: 1.3em; margin-top: 0.4em; flex-shrink: 0; position: relative;'><section style='position: absolute; top: -3px; left: -4px; width: 1.1em; height: 1.15em; background: #c49670; border-radius: 51% 49% 52% 48%; opacity: 0.25;'></section><span style='width: 0.65em; height: 0.68em; background: #7a4f2e; border-radius: 49% 51% 48% 52%; display: inline-block; box-shadow: 0 1.5px 5px rgba(122, 79, 46, 0.35), inset 0 0.5px 0 rgba(255, 255, 255, 0.15); position: relative; z-index: 1; margin-left: 0.02em;'></span></section><span style='flex: 1; margin-left: 0.02em;'>{{content}}</span></li>"
      },
      "checklist": {
        "wrapper": "<ul style='margin: 2em 0.02em; padding-left: 0; list-style: none; background: #f8f4ee; background-image: radial-gradient(ellipse 70px 85px at 48% 52%, rgba(196, 150, 112, 0.08) 0%, transparent 55%); border: 2px solid #c49670; border-radius: 17px 14px 16px 15px; padding: 1.4em 1.6em 1.6em 1.7em; box-shadow: 0 4px 16px rgba(122, 79, 46, 0.08), inset 0 0.5px 0 rgba(255, 255, 255, 0.28); position: relative;'>{{items}}</ul>",
        "item": "<li style='padding: 0.9em 0 1.1em 0.06em; line-height: 1.75; display: flex; align-items: flex-start; color: #5a4930; position: relative; border-bottom: 1px dashed #c49670; margin-bottom: 0.6em;'><section style='margin-right: 1.2em; margin-top: 0.15em; width: 1.5em; height: 1.55em; border-radius: 6px 8px 5px 7px; display: flex; align-items: center; justify-content: center; flex-shrink: 0; background: #fefdf9; box-shadow: 0 2.5px 8px rgba(122, 79, 46, 0.18), inset 0 1px 3px rgba(196, 150, 112, 0.08); border: 2px solid #c49670; position: relative;'><section style='position: absolute; top: -2px; right: -4px; width: 6px; height: 7px; background: #7a4f2e; border-radius: 52% 48% 51% 49%; opacity: 0.55;'></section>{{checkbox}}</section><span style='flex: 1; margin-top: 0.08em; margin-left: 0.02em;'>{{content}}</span></li>",
        "checked": "<span style='color: #7a4f2e; font-weight: 750; font-size: 0.9em;'>✓</span>",
        "unchecked": ""
      }
    },
    "raw": "{{html}}"
  }
