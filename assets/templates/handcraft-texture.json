    "b": "font-weight: 650; color: #7a4f2e; background: #f9f5ef; padding: 0.15em 0.35em 0.2em 0.4em; border-radius: 7px 4px 6px 8px; border: 1.5px solid #c49670; box-shadow: 1px 2px 3px rgba(122, 79, 46, 0.15); margin: 0 0.1em; position: relative;",
    "em": "font-style: italic; color: #8f5c3a; font-weight: 500; background: #fcf9f3; padding: 0.1em 0.25em 0.15em 0.35em; border-radius: 5px 8px 4px 6px; border: 1px dashed #d1a17a; margin: 0 0.05em; position: relative;",
    "i": "font-style: italic; color: #8f5c3a; font-weight: 500; background: #fcf9f3; padding: 0.1em 0.25em 0.15em 0.35em; border-radius: 5px 8px 4px 6px; border: 1px dashed #d1a17a; margin: 0 0.05em; position: relative;",
    "u": "text-decoration: none; color: #795238; background: linear-gradient(to right, transparent 2%, #c49670 8%, #d1a17a 15%, #c49670 85%, #d1a17a 92%, transparent 98%); background-size: 100% 0.18em; background-position: 0 82%; background-repeat: no-repeat; padding-bottom: 0.12em;",
    "code": "font-family: 'Monaco', 'Menlo', monospace; font-size: 0.88em; background: #2a2723; color: #c49670; padding: 0.25em 0.5em 0.3em 0.6em; border-radius: 6px 9px 5px 7px; border: 1.5px solid #7a4f2e; font-weight: 550; box-shadow: inset 1px 1px 3px rgba(0,0,0,0.4), 0 1px 3px rgba(122, 79, 46, 0.2); margin: 0 0.1em;",
    "mark": "background: #ffeac7; color: #8b5a2f; padding: 0.15em 0.3em 0.2em 0.4em; border-radius: 8px 5px 7px 4px; font-weight: 580; border: 1px solid #deb887; box-shadow: 0 1px 2px rgba(139, 90, 47, 0.25); margin: 0 0.08em;",
    "a": "color: #8f5c3a; text-decoration: none; font-weight: 580; background: #fcf9f3; padding: 0.08em 0.25em 0.12em 0.3em; border-radius: 5px 7px 4px 6px; border-bottom: 1.8px solid #c49670; box-shadow: 0 1px 2px rgba(143, 92, 58, 0.18); margin: 0 0.05em;",
    "sup": "color: #7a4f2e; font-size: 0.72em; font-weight: 580; vertical-align: super; background: #f9f5ef; padding: 0.12em 0.2em 0.15em 0.25em; border-radius: 4px 6px 3px 5px; border: 1px solid #c49670; margin: 0 0.03em;"
  },
  "globalStyles": {
    "container": "<section style='max-width: 100%; margin: 0 auto; padding: 2.2em 1.8em 2.5em 2em; font-size: 16px; line-height: 1.75; color: #5a4930; font-family: \"Georgia\", \"Palatino\", \"Times New Roman\", serif; background: #fefdf9; background-image: radial-gradient(ellipse 60px 90px at 25% 30%, rgba(196, 150, 112, 0.03) 0%, transparent 40%), radial-gradient(ellipse 45px 68px at 75% 70%, rgba(122, 79, 46, 0.025) 0%, transparent 35%), linear-gradient(135deg, rgba(209, 161, 122, 0.015) 0%, transparent 30%), linear-gradient(-25deg, rgba(196, 150, 112, 0.012) 0%, transparent 40%); border: 2px solid #c49670; border-radius: 14px 9px 16px 12px; box-shadow: 0 5px 22px rgba(122, 79, 46, 0.1), inset 0 0.5px 0 rgba(255, 255, 255, 0.2); position: relative;'>{{content}}</section>",
    "references": "<section style='margin-top: 3em; padding: 1.8em 2em 2em 1.8em; background: #f8f4ee; background-image: radial-gradient(ellipse 55px 80px at 18% 75%, rgba(196, 150, 112, 0.06) 0%, transparent 45%), radial-gradient(ellipse 40px 60px at 82% 25%, rgba(122, 79, 46, 0.05) 0%, transparent 40%); border: 2px solid #c49670; border-radius: 18px 13px 15px 14px; font-family: inherit; box-shadow: 0 3px 14px rgba(122, 79, 46, 0.06), inset 0 0.5px 0 rgba(255, 255, 255, 0.3); position: relative;'><section style='position: absolute; top: -6px; left: 22px; background: #7a4f2e; color: #fefdf9; padding: 0.35em 1.1em 0.4em 1.3em; border-radius: 13px 9px 11px 10px; font-size: 0.82em; font-weight: 650; box-shadow: 0 2.5px 8px rgba(122, 79, 46, 0.2);'>参考资料</section><section style='margin-top: 1.1em; font-size: 0.88em; line-height: 1.65; color: #5a4930;'>{{links}}</section></section>"
  },
  "blocks": {
    "paragraph": "<p style='font-size: 1em; color: #5a4930; margin: 0 0 1.5em 0.12em 0 0.04em; line-height: 1.8; font-family: inherit; text-align: left; font-weight: 420; text-indent: 1.5em; position: relative; margin-left: 0.1em;'>{{text}}</p>",
    "header": {
      "h1": "<section style='margin: 2.2em 0.15em 1.8em 0.08em; text-align: center; position: relative;'><section style='position: absolute; top: -8px; left: 52%; width: 60px; height: 18px; background: #c49670; border-radius: 32px 35px 30px 34px; opacity: 0.2; margin-left: -30px;'></section><h1 style='font-size: 1.6em; font-weight: 680; line-height: 1.2; color: #7a4f2e; font-family: \"Georgia\", \"Palatino\", serif; margin: 0; padding: 0.8em 1.2em 1em 1.4em; background: #fefdf9; background-image: radial-gradient(ellipse 120% 80% at 45% 55%, rgba(196, 150, 112, 0.06) 0%, transparent 65%); border: 2px solid #c49670; border-radius: 16px 11px 18px 13px; box-shadow: 0 4px 15px rgba(122, 79, 46, 0.15), inset 0 1px 0 rgba(255, 255, 255, 0.25); position: relative; text-shadow: 0 0.5px 1.5px rgba(122, 79, 46, 0.06);'>{{text}}</h1></section>",
      "h2": "<section style='margin: 1.8em 0.08em 1.2em 0.04em; position: relative;'><section style='position: absolute; left: -5px; top: 48%; width: 4px; height: 36px; background: #c49670; border-radius: 3px 2px 3px 4px; margin-top: -18px; box-shadow: 1px 0 2px rgba(196, 150, 112, 0.22);'></section><h2 style='font-size: 1.4em; font-weight: 620; line-height: 1.3; color: #7a4f2e; font-family: \"Georgia\", \"Palatino\", serif; margin: 0; padding: 0.6em 1em 0.7em 1.3em; background: #f8f4ee; background-image: linear-gradient(138deg, rgba(196, 150, 112, 0.05) 0%, transparent 95%); border: 2px solid #c49670; border-left: none; border-radius: 0 12px 14px 0; position: relative; box-shadow: 0 2px 8px rgba(122, 79, 46, 0.06), inset 0 0.5px 0 rgba(255, 255, 255, 0.2);'>{{text}}</h2></section>",
      "h3": "<section style='margin: 1.5em 0.06em 0.9em 0.1em; position: relative;'><section style='position: absolute; right: -3px; top: -4px; width: 12px; height: 13px; background: #7a4f2e; border-radius: 47% 49% 46% 50%; box-shadow: 0 1px 3px rgba(122, 79, 46, 0.3);'></section><h3 style='font-size: 1.2em; font-weight: 580; line-height: 1.4; color: #5a4930; font-family: \"Georgia\", \"Palatino\", serif; margin: 0; padding: 0.6em 0.8em 0.7em 1em; background: #fcf9f3; background-image: radial-gradient(ellipse 80% 60% at 25% 30%, rgba(196, 150, 112, 0.035) 0%, transparent 55%); border: 1.5px dashed #c49670; border-radius: 11px 9px 12px 10px; position: relative; box-shadow: 0 2px 5px rgba(122, 79, 46, 0.05);'>{{text}}</h3></section>"
    },
    "quote": {
      "template": "<section style='margin: 2.6em 0.08em; position: relative;'><section style='position: absolute; top: -6px; left: -9px; width: 26px; height: 28px; background: #c49670; border-radius: 45% 49% 44% 50%; opacity: 0.15;'></section><section style='position: absolute; bottom: -5px; right: -5px; width: 17px; height: 18px; background: #7a4f2e; border-radius: 48% 46% 49% 45%; opacity: 0.25;'></section><section style='padding: 1.9em 2.2em 2.1em 2.4em; background: #f8f4ee; background-image: radial-gradient(ellipse 70px 90px at 32% 68%, rgba(196, 150, 112, 0.06) 0%, transparent 48%), linear-gradient(142deg, rgba(122, 79, 46, 0.02) 0%, transparent 95%); border: 2px solid #c49670; border-radius: 19px 14px 21px 16px; font-family: inherit; position: relative; z-index: 1; box-shadow: 0 5px 20px rgba(122, 79, 46, 0.1), inset 0 1px 0 rgba(255, 255, 255, 0.3);'><section style='position: absolute; top: 10px; left: 14px; font-size: 2.2em; color: #c49670; opacity: 0.2; font-family: serif; line-height: 1; margin-left: 0.08em;'>&ldquo;</section><p style='margin: 0.3em 0 1.1em 0; color: #5a4930; font-style: italic; font-size: 1.05em; line-height: 1.7; font-weight: 480; text-align: center; position: relative; z-index: 2; margin-left: 0.06em;'>{{text}}</p>{{?caption}}</section></section>",
      "optional": {
        "caption": "<p style='text-align: center; font-size: 0.9em; color: #7a4f2e; font-style: normal; margin: 0.9em 0 0 0; font-weight: 580; position: relative; z-index: 2; margin-left: 0.04em;'>— {{value}}</p>"
      }
    },
    "delimiter": "<section style='display: flex; align-items: center; justify-content: center; margin: 2.8em 0.1em; position: relative;'><section style='position: absolute; left: 20%; width: 10px; height: 11px; background: #c49670; border-radius: 52% 48% 51% 49%; opacity: 0.35;'></section><section style='position: absolute; right: 25%; width: 9px; height: 10px; background: #7a4f2e; border-radius: 49% 51% 48% 52%; opacity: 0.42;'></section><span style='width: 2.8em; height: 0.16em; background: #c49670; border-radius: 0.08em; margin: 0 0.6em; box-shadow: 0 1px 2px rgba(196, 150, 112, 0.35); margin-left: 0.65em;'></span><span style='width: 1.1em; height: 1.15em; background: #7a4f2e; border-radius: 52% 48% 49% 51%; margin: 0 0.5em; box-shadow: 0 2px 6px rgba(122, 79, 46, 0.38), inset 0 0.5px 0 rgba(255, 255, 255, 0.15); margin-right: 0.6em;'></span><span style='width: 2.8em; height: 0.16em; background: #c49670; border-radius: 0.08em; margin: 0 0.6em; box-shadow: 0 1px 2px rgba(196, 150, 112, 0.35); margin-right: 0.7em;'></span></section>",
    "image": {
      "template": "<section style='margin: 2.5em 0.08em; text-align: center; position: relative;'><section style='position: absolute; top: -10px; left: 25%; width: 22px; height: 23px; background: #c49670; border-radius: 51% 49% 48% 52%; opacity: 0.28;'></section><section style='display: inline-block; border-radius: 16px 14px 18px 15px; overflow: hidden; box-shadow: 0 6px 24px rgba(122, 79, 46, 0.18); background: #fefdf9; padding: 0.8em 1em 0.9em 0.85em; position: relative; z-index: 1; border: 2px solid #c49670;'><section style='border-radius: 12px 14px 11px 13px; overflow: hidden; position: relative; box-shadow: inset 0 1px 3px rgba(122, 79, 46, 0.08);'><img src='{{url}}' alt='{{alt}}' style='max-width: 100%; display: block;' /></section></section>{{?caption}}</section>",
      "optional": {
        "caption": "<p style='margin: 1.5em 0.05em 0 0.02em; font-size: 0.88em; color: #7a4f2e; font-style: italic; line-height: 1.6; text-align: center; background: #f8f4ee; padding: 0.7em 1.2em 0.8em 1.4em; border-radius: 15px 13px 17px 14px; display: inline-block; font-weight: 520; border: 2px solid #c49670; box-shadow: 0 3px 12px rgba(122, 79, 46, 0.12);'>{{value}}</p>"
      }
    },
    "code": "<section style='margin: 2.5em 0.05em; position: relative; overflow: hidden;'><section style='position: absolute; top: -7px; right: -6px; width: 25px; height: 26px; background: #7a4f2e; border-radius: 48% 52% 49% 51%; opacity: 0.22;'></section><section style='background: #2a2723; background-image: radial-gradient(ellipse 80px 60px at 78% 22%, rgba(196, 150, 112, 0.08) 0%, transparent 45%); border: 2px solid #7a4f2e; border-radius: 15px 13px 17px 14px; overflow: hidden; box-shadow: 0 6px 20px rgba(122, 79, 46, 0.25), inset 0 1px 3px rgba(0,0,0,0.35); position: relative; z-index: 1;'><section style='background: #7a4f2e; background-image: linear-gradient(142deg, rgba(196, 150, 112, 0.15) 0%, transparent 90%); padding: 0.8em 1.2em 0.9em 1.4em; display: flex; align-items: center; justify-content: space-between; border-bottom: 2px solid #5a4930;'><section style='display: flex; align-items: center;'><span style='width: 0.7em; height: 0.75em; background: #c49670; border-radius: 48% 52% 51% 49%; margin-right: 0.6em; box-shadow: 0 1.5px 5px rgba(196, 150, 112, 0.35), inset 0 0.5px 0 rgba(255, 255, 255, 0.18); margin-left: 0.03em;'></span><span style='width: 0.7em; height: 0.75em; background: #f8f4ee; border-radius: 52% 48% 49% 51%; margin-right: 0.6em; box-shadow: 0 1.5px 5px rgba(248, 244, 238, 0.35), inset 0 0.5px 0 rgba(255, 255, 255, 0.25); margin-left: 0.02em;'></span><span style='width: 0.7em; height: 0.75em; background: #fcf9f3; border-radius: 49% 51% 52% 48%; box-shadow: 0 1.5px 5px rgba(252, 249, 243, 0.35), inset 0 0.5px 0 rgba(255, 255, 255, 0.25); margin-right: 0.03em;'></span></section><span style='font-size: 0.8em; color: #fefdf9; font-weight: 580; text-transform: uppercase; letter-spacing: 0.08em; text-shadow: 0 1px 2px rgba(0,0,0,0.28); margin-right: 0.08em;'>代码</span></section><section style='padding: 1.8em 1.8em 2em 1.6em; font-family: \"Monaco\", \"Menlo\", monospace; font-size: 0.88em; line-height: 1.6; color: #c49670; background: #2a2723; overflow-x: auto;'><span style='font-family: inherit; background: transparent; color: inherit; font-weight: 520;'>{{code}}</span></section></section></section>",
    "list": {
      "ordered": {
        "wrapper": "<ol style='margin: 2em 0.08em; padding-left: 0; list-style: none; background: #f8f4ee; background-image: radial-gradient(ellipse 60px 90px at 12% 88%, rgba(196, 150, 112, 0.06) 0%, transparent 48%); border: 2px solid #c49670; border-radius: 16px 14px 18px 15px; padding: 1.4em 1.6em 1.6em 1.5em; box-shadow: 0 4px 16px rgba(122, 79, 46, 0.08), inset 0 0.5px 0 rgba(255, 255, 255, 0.28); position: relative;'>{{items}}</ol>",
